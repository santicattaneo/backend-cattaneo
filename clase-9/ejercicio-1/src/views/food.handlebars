<div>
    <h1>Hola {{user.name}}, bienvenido!</h1>
    {{#if isAdmin}}
    {{#each food}}
    <div>
        <p>{{this.name}}</p>
        <p>{{this.price}}</p>
    </div>
    {{/each}}
    {{else}}
    <p>No tienes permisos para ver los productos</p>
    {{/if}}
</div>
<script src="/js/index.js"></script>